.root{
    display: flex;
    flex-direction: column;
    background-color: white;
    align-items: center;
    width: 100vw;
}

.banner{
  width: 100vw;
  height: calc(100vw*383/900);
}


.itembg {
  width: 100vw;
  height: 200rpx;
  background-image: url('data:image/png;base64,');
  background-repeat:no-repeat;
  background-size:100% 100%;
}

.business{
  width: 92vw;
  margin-left: 4vw;
  height: 180rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  background-color: white;
}

.businessItem{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  background-color: white;
  border: 0;
}

.businessImage{
  width: 60px;
  height: 60px;
}

.businessText{
  font-size: 11pt;
  color: #666;
}


.cell-head{
  padding: 20rpx;
  display: flex;
  align-self: stretch;
  flex-direction: row;
  justify-content: flex-start;
  border-bottom: solid #efefef 1px;
}

.cell-head-color {
  background-color: #1567b9;
  height: 40rpx;
  width: 10rpx;
  border-radius: 4rpx;
}

.cell-head-text{
  margin-left: 20rpx;
  color: #54a0ff;
  font-weight: bold;
  font-size: 11pt;
}

.line{
  background-color: #efefef;
  width: 100vw;
  height: 20rpx;
}

.cell-content-view {
  display: flex;
  flex: 1;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  width:100vw;
}

.cell-item{
  flex: 1;
  width: 25vw;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.inner-image {
  width: 25vw;
  height: 25vw;
}

.cell-content-view-text{
  margin-top: 6px;
  font-size: 11pt;
  color: #666;
}

.cell-content-colum{
  display: flex;
  flex-direction: column;
  width: 100vw;
  align-items: center;
  margin-bottom: 20rpx;
}

.cell-content-view-image{
  width: 90vw;
  margin-top: 20rpx;
  height: 38.3vw;
  border-radius: 8rpx;
}

